<!DOCTYPE module SYSTEM "module.dtd">

<module name="xbee_resetter">
  <doc>
    <description>XBee Resetter</description>
  </doc>
  <header>
    <file name="xbee_resetter.h"/>
  </header>
  <init fun="init_xbee_resetter()"/>
  <periodic fun="periodic_1Hz_xbee_resetter()" freq="1." start="start_xbee_resetter()" stop="stop_xbee_resetter()" autorun="TRUE"/>
	<makefile>
    <file name="xbee_resetter.c"/>
  </makefile>
</module>

